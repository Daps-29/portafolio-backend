{
  "name": "portafolio-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run copy:statics",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npm run copy:statics",
    "start": "nest start",
    "predev": "npm run copy:statics",
    "dev": "NODE_ENV=development nest start --watch",
    "setup": "npm run migrations:clean && npm run schema:drop && npm run migrations:generate database/migrations/init && npm run migrations:run && npm run seeds:run",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "NODE_OPTIONS=--max_old_space_size=8192 ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migrations:clean": "rimraf database/migrations/*",
    "migrations:generate": "npm run typeorm -- migration:generate -d ormconfig-default.ts",
    "migrations:run": "npm run typeorm -- migration:run -d ormconfig-default.ts",
    "migrations:revert": "npm run typeorm -- migration:revert -d ormconfig-default.ts",
    "seeds:create": "npm run typeorm -- migration:create",
    "seeds:run": "npm run typeorm -- migration:run -d ormconfig-seed.ts",
    "schema:drop": "npm run typeorm -- schema:drop -d ormconfig-default.ts",
    "rimraf": "./node_modules/rimraf/bin.js",
    "copy:statics": "mkdir -p dist/src/core/config/authorization && cp -pv ./src/core/config/authorization/*.conf dist/src/core/config/authorization/"
  },
  "dependencies": {
    "@elartedelcodigo/check-node-version": "1.1.7",
    "@golevelup/ts-jest": "0.7.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.6",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/passport": "11.0.5",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/schedule": "6.0.0",
    "@nestjs/typeorm": "11.0.0",
    "address": "2.0.3",
    "bcrypt": "6.0.0",
    "class-transform": "0.7.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cls-rtracer": "2.6.3",
    "connect-typeorm": "2.0.0",
    "cookie-parser": "1.4.7",
    "dayjs": "1.11.13",
    "dotenv": "17.2.1",
    "exceljs": "^4.4.0",
    "file-stream-rotator": "1.0.0",
    "helmet": "8.1.0",
    "multer": "^2.0.2",
    "nanoid": "5.1.5",
    "nestjs-pino": "^4.4.0",
    "nodemailer": "^7.0.5",
    "openid-client": "6.6.4",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "pg": "8.16.3",
    "pino": "9.9.0",
    "pino-loki": "2.6.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "sql-formatter": "15.6.6",
    "typeorm": "0.3.25",
    "typeorm-adapter": "1.7.0",
    "uuid": "11.1.0",
    "zxcvbn-typescript": "5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.33.0",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.7",
    "@nestjs/testing": "11.1.6",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.13.3",
    "@types/express": "5.0.3",
    "@types/jest": "30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.0",
    "@types/supertest": "6.0.3",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "8.40.0",
    "@typescript-eslint/parser": "8.40.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.4.0",
    "globals": "16.3.0",
    "jest": "30.0.5",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "source-map-support": "0.5.21",
    "supertest": "7.1.4",
    "ts-jest": "29.4.1",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.9.2"
  },
  "overrides": {
    "axios": ">=1.8.2",
    "form-data": ">=4.0.4",
    "brace-expansion": ">=2.0.2",
    "tmp": ">=0.2.4",
    "on-headers": ">=1.1.0",
    "@babel/helpers": ">=7.26.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coverageReporters": [
      "html",
      "text",
      "text-summary",
      "cobertura"
    ],
    "coveragePathIgnorePatterns": [
      "src/main.ts",
      "src/database/migrations/",
      "src/database/seeds/",
      ".module.ts",
      ".entity.ts",
      ".dto.ts",
      ".guard.ts",
      ".enum.ts"
    ],
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    }
  },
  "jest-junit": {
    "outputDirectory": "./coverage"
  },
  "engines": {
    "node": "^22",
    "npm": ">=10"
  }
}
