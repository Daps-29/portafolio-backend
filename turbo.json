{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build-docker": {
      "dependsOn": ["^build-docker"],
      "outputs": ["dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  },
  "remoteCache": {
    "enabled": true
  }
}
